<table id="table" class="table table-hover" style="text-align: center; vertical-align: middle;">
    <thead>
        <tr>
            <th style="display:none;" scope="col">#</th>
            <th scope="col">#</th>
            <th scope="col">เลขที่สัญญา</th>
            <th scope="col">ชื่อ</th>
            <th scope="col">นามสกุล</th>
            <th scope="col">สถานที่</th>
            <th scope="col">สถานะ</th>
            <th scope="col">ประเภท</th>
            @if (ViewBag.UserAccess == StatusUserAccess.UserAccess_Superadmin.ToString())
            {
                <th scope="col">บ้าน</th>
            }
            @if (int.Parse(ViewBag.UserAccess) <= StatusUserAccess.UserAccess_Audit)
            {
                <th scope="col">สาย</th>
            }
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @if (ViewBag.PageData.Count > 0)
        {
            var i = 1;
            foreach (var items in ViewBag.PageData)
            {
                var redcard = (items.Status == ContractStatus.StatusContract_WaitConfirm ? "btn-danger" : "");
                <tr class='clickable-row @redcard'>
                    <th style="display:none;" scope="row">@items.Id</th>
                    <th>@i</th>
                    <td>@items.ContractId</td>
                    <td>@items.Customer.Firstname</td>
                    <td>@items.Customer.Lastname</td>
                    <td>@items.Customer.Address</td>
                    <td>@items.StatusText</td>
                    <td>@items.TypeText</td>
                    @if (ViewBag.UserAccess == StatusUserAccess.UserAccess_Superadmin.ToString())
                    {
                        <td>@items.Customer.HouseText</td>
                    }
                    @if (int.Parse(ViewBag.UserAccess) <= StatusUserAccess.UserAccess_Audit)
                    {
                        <td>@items.Customer.CustomerLineText</td>
                    }
                    <td>
                        <button type="button" class="btn btn-primary btn-sm btn-view" style="border:solid 1px #fff;" data-bs-toggle="modal" data-bs-target="#viewcontract">
                            <i class="fa fa-eye"></i>
                            ดู
                        </button>
                        @if (int.Parse(ViewBag.UserAccess) <= StatusUserAccess.UserAccess_Admin && ContractStatus.StatusContract_WaitConfirm != items.Status)
                        {
                            <button type="button" class="btn btn-warning btn-sm btn-edit" style="border:solid 1px #fff;" data-bs-toggle="modal" data-bs-target="#editcontract">
                                <i class="fa fa-pen-square"></i>
                                แก้ไข
                            </button>
                        }
                        @if (int.Parse(ViewBag.UserAccess) <= StatusUserAccess.UserAccess_Admin && ContractStatus.StatusContract_WaitConfirm == items.Status)
                        {
                            <button type="button" class="btn btn-warning btn-sm btn-editcontractwait" style="border:solid 1px #fff;" data-bs-toggle="modal" data-bs-target="#editcontractwait">
                                <i class="fa fa-pen-square"></i>
                                ดูคำขอ
                            </button>
                        }
                        @if (int.Parse(ViewBag.UserAccess) == StatusUserAccess.UserAccess_Agent && ContractStatus.StatusContract_Normal == items.Status)
                        {
                            <button type="button" class="btn btn-warning btn-sm btn-editcontractnormal" style="border:solid 1px #fff;" data-bs-toggle="modal" data-bs-target="#editcontractnormal">
                                <i class="fa fa-pen-square"></i>
                                ใส่หลักฐาน
                            </button>
                        }
                        @if (ViewBag.UserAccess == StatusUserAccess.UserAccess_Superadmin.ToString())
                        {
                            <button type="button" class="btn btn-danger btn-sm btn-delete" style="border:solid 1px #fff;" data-bs-toggle="modal" data-bs-target="#delcontract">
                                <i class="fa fa-trash-alt"></i>
                                ลบ
                            </button>
                        }
                    </td>
                </tr>
                i++;
            }
        }
    </tbody>
