@using DailyLoan.Library.Status

@switch (ViewBag.partialView)
{
    case "Warn":
        <partial name="Warn.cshtml" />
        break;
    case "Customer":
        <partial name="Customer.cshtml" />
        break;
    case "Contract":
        <partial name="Contract.cshtml" />
        break;
    case "DailyReport":
        <partial name="DailyReport.cshtml" />
        break;
    case "History":
        <partial name="History.cshtml" />
        break;
    case "Collector":
        <partial name="Collector.cshtml" />
        break;
    case "setting_home":
        <partial name="setting_home.cshtml" />
        break;
    case "setting_daily":
        <partial name="setting_daily.cshtml" />
        break;
    case "setting_system":
        <partial name="setting_system.cshtml" />
        break;
}