@using DailyLoan.Library.Status
<br>
<div class="accordion" id="accordionPanelsStayOpenExample">
    <div class="accordion-item">
        <h2 class="accordion-header" id="panelsStayOpen-headingOne">
            <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapseOne" aria-expanded="false" aria-controls="panelsStayOpen-collapseOne" style="background-color">
                <b>บ้าน</b>
            </button>
        </h2>
        <div id="panelsStayOpen-collapseOne" class="accordion-collapse collapse show" aria-labelledby="panelsStayOpen-headingOne">
            <div class="accordion-body">
                <form>
                    <div class="d-flex justify-content-end" ">
                        <button type="button" class="btn btn-success btn-sm" data-bs-toggle="modal" data-bs-target="#addhouse">
                            <i class="fa fa-plus-circle"></i>
                            เพิ่มบ้าน
                        </button>
                    </div>
                    <br>
                    <div class="form-row">
                        <div class="container-fluid">
                            <div class="row">
                                <table class="table table-bordered table-hover" style="text-align: center; vertical-align: middle;">
                                    <thead>
                                        <tr>
                                            <th scope="col">#</th>
                                            <th scope="col">บ้าน</th>
                                            <th scope="col">จังหวัด</th>
                                            <th scope="col">อำเภอ</th>
                                            <th scope="col"></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (ViewBag.House.Count > 0)
                                        {
                                            foreach (var items in ViewBag.House)
                                            {
                                                <tr class='clickable-row'>
                                                    <th scope="row">@items.Id</th>
                                                    <td>@items.HouseName</td>
                                                    <td>@items.Province</td>
                                                    <td>@items.District</td>
                                                    <!--
                                                    <td>@items.SubDistrict</td>
                                                    <td>@items.StatusText</td>
                                                    <td>@items.Remark</td>
                                                      -->
                                                    <td>
                                                        <button type="button" class="btn btn-warning btn-sm" data-bs-toggle="modal" data-bs-target="#edithouse">
                                                            <i class="fa fa-pen-square"></i>
                                                            แก้ไข
                                                        </button>
                                                        <button type="button" class="btn btn-danger btn-sm" data-bs-toggle="modal" data-bs-target="#deletehouse">
                                                            <i class="fa fa-trash-alt"></i>
                                                            ลบ
                                                        </button>
                                                    </td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="accordion-item">
        <h2 class="accordion-header" id="panelsStayOpen-headingTwo_1">
            <button class="accordion-button " type="button" data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapseTwo_1" aria-expanded="false" aria-controls="panelsStayOpen-collapseTwo_1">
                <b>สาย</b>
            </button>
        </h2>
        <div id="panelsStayOpen-collapseTwo_1" class="accordion-collapse collapse show" aria-labelledby="panelsStayOpen-headingTwo_1">
            <div class="accordion-body">
                <form>
                    <div class="d-flex justify-content-end" ">
                        <button type="button" class="btn btn-success btn-sm" data-bs-toggle="modal" data-bs-target="#addCustomerLine">
                            <i class="fa fa-plus-circle"></i>
                            เพิ่มสาย
                        </button>
                    </div>
                    <br>
                    <div class="form-row">
                        <div class="container-fluid">
                            <div class="row">
                                <table class="table table-bordered table-hover" style="text-align: center; vertical-align: middle;">
                                    <thead>
                                        <tr>
                                            <th scope="col">#</th>
                                            <th scope="col">บ้าน</th>
                                            <th scope="col">สาย</th>
                                            <th scope="col"></th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        @if (ViewBag.PageData.Count > 0)
                                        {
                                            for (var i=0;i<ViewBag.PageData.Count;i++)
                                            {
                                                var items = ViewBag.PageData[i];
                                    <tr class='clickable-row'>
                                        <th scope="row">@items.Id</th>
                                        @if (ViewBag.UserAccess == StatusUserAccess.UserAccess_Superadmin.ToString())
                                        {
                                            <td>@items.HouseText</td>
                                        }
                                        <td>@items.CustomerLineName</td>
                                        <!-- -- >
    <td>@items.StatusText</td>
    <td>@items.Remark</td>
    <!-- -->
                                        <td>
                                            <button type="button" class="btn btn-warning btn-sm" data-bs-toggle="modal" data-bs-target="#editCustomerLine">
                                                <i class="fa fa-pen-square"></i>
                                                แก้ไข
                                            </button>
                                            <button type="button" class="btn btn-danger btn-sm" data-bs-toggle="modal" data-bs-target="#deleteCustomerLine">
                                                <i class="fa fa-trash-alt"></i>
                                                ลบ
                                            </button>

                                        </td>
                                    </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<!-- The Modal addhouse -->
<div class="modal" id="addhouse">
    <div class="modal-dialog modal-md">
        <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header" style="background-color:#ff9800;color:#000;">
                <h4 class="modal-title">เพิ่มบ้าน</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
            </div>

            <!-- Modal body -->
            <div class="modal-body">
                <form>
                    <div class="form-row">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col pb-3">
                                    <label for="inputEmail4">บ้าน</label>
                                    <input type="text" class="form-control form-control-sm" id="inputEmail4" placeholder="บ้าน">
                                </div>
                            </div>
                            <div class="row">
                                <div class="col pb-3">
                                    <label for="house" class="form-label">จังหวัด</label>
                                    <select class="form-select form-select-sm" id="house" name="sellist1">
                                        <option>จังหวัด1</option>
                                        <option>จังหวัด2</option>
                                    </select>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col pb-3">
                                    <label for="house_detail" class="form-label">อำเภอ</label>
                                    <select class="form-select form-select-sm" id="house_detail" name="sellist1">
                                        <option>อำเภอ1</option>
                                        <option>อำเภอ2</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>

            <!-- Modal footer -->
            <div class="modal-footer">
                <button type="button" class="btn btn-success" data-bs-dismiss="modal">บันทึก</button>
                <button type="button" class="btn btn-danger" data-bs-dismiss="modal">ปิด</button>
            </div>

        </div>
    </div>
</div>
<!-- end -->
